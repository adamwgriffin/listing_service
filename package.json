{
  "name": "listing_service",
  "version": "0.0.1",
  "description": "A service for searching real estate listings",
  "main": "dist/server.js",
  "author": "Adam Griffin",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "build": "tsc",
    "test": "docker compose run --rm -e DB_HOST=mongodb-test app yarn jest",
    "console": "docker compose run --rm app yarn ts-node",
    "generate_listing_data": "./run-script src/bin/generate_listing_data",
    "seed_listing_data": "./run-script src/bin/seed_listing_data",
    "seed_boundary_data": "./run-script src/bin/seed_boundary_data",
    "seed_dev_data": "./run-script src/bin/seed_boundary_data && ./run-script src/bin/seed_listing_data",
    "generate_and_seed_listing_data": "./run-script src/bin/generate_listing_data && ./run-script src/bin/seed_listing_data"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.28",
    "@koa/router": "^12.0.0",
    "@turf/turf": "^6.5.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.0",
    "mongoose": "^7.0.3",
    "slugify": "^1.6.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "exponential-backoff": "^3.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.5.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "yargs": "^17.7.2"
  }
}
