{
  "name": "listing_service",
  "version": "0.0.1",
  "description": "A service for searching real estate listings",
  "main": "dist/server.js",
  "author": "Adam Griffin",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "build": "tsc",
    "test": "docker compose up -d app-test && docker compose exec app-test npx jest --runInBand",
    "console": "docker compose run --rm app npx tsx",
    "generate_listing_data": "./run-script src/bin/generate_listing_data.ts",
    "seed_listing_data": "./run-script src/bin/seed_listing_data.ts",
    "seed_boundary_data": "./run-script src/bin/seed_boundary_data.ts",
    "seed_dev_data": "npm run seed_boundary_data && npm run seed_listing_data",
    "generate_and_seed_listing_data": "npm run generate_listing_data && npm run seed_listing_data",
    "clear_cache": "./run-script src/bin/clear_cache"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@keyv/redis": "^5.0.0",
    "@koa/router": "^13.1.1",
    "@turf/turf": "^7.2.0",
    "cache-manager": "^7.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.4.1",
    "mongoose": "^8.17.0",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "slugify": "^1.6.6",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@faker-js/faker": "^9.9.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/koa": "^3.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.32.0",
    "exponential-backoff": "^3.1.1",
    "jest": "^30.0.5",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "yargs": "^18.0.0"
  }
}
